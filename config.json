{
  "engine_config": "../engines/goldstar-ds/engine.json",

  "splitting": {
    "version": "1",
    "source_file": "/tmp/goldstar_analysis/data_2year_1sig",
    "train_file": "/tmp/goldstar_analysis/train",
    "test_file": "/tmp/goldstar_analysis/test",
    "type": "date",
    "train_ratio": 0.8,
    "random_seed": 29750,
    "split_event": "purchased-event"
  },

  "reporting": {
    "file": "./report.xlsx",
    "csv_dir": "./report",
    "use_uuid": true
  },

  "testing": {
    "map_k": 10,
    "non_zero_users_file": "./non_zero_users.dat",
    "consider_non_zero_scores_only": true,
    "custom_combos": {
      "event_groups": [["mm-feedback-like"]]
    }
  },

  "spark": {
    "master": "local[4]"
  }
}
